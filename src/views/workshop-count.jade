extends layout


block content
  footer
  section#pricing-table.section-page
    .container
      h2.title-list(style='font-size:40px;') PRE-INSCRITOS TALLER - CIIS
      .table-responsive
        table#listPM.table.table-prices
          thead
            tr
              td
                | Taller
              td
                | Inscritos
          if   user
            tbody
              each users in user
                tr
                  td
                    h4  #{users.name}
                  td
                    h4  #{users.count}
                  ul
      button.list-button(onclick="tableToExcel('listPM','POSTMASTER : LISTA DE PRE-INSCRITOS')") EXPORTAR
